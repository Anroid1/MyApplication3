<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.WodeFragment"
    android:background="@color/white">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/r1"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/dzl1"
            android:background="#449e86"
            >

        <ImageView
            android:id="@+id/touxiang"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/touxiang" />

            <TextView
                android:id="@+id/nicheng"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_below="@+id/touxiang"
                android:layout_marginTop="-1dp"
                android:text="世界港口"
                android:layout_centerHorizontal="true"
                android:textColor="@color/white"
                android:textSize="18dp" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/dzl1"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/dingdan"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:src="@drawable/dingdan"></ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:gravity="center"
                android:layout_toRightOf="@+id/dingdan"
                android:text="全部订单"
                android:textColor="#101010"
                android:textSize="18dp"></TextView>

            <ImageView
                android:id="@+id/image2"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="22dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/button1"></ImageView>
        </RelativeLayout>

        <View
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginTop="1px"
            android:layout_below="@+id/rl2"
            android:background="#CECECE" />

        <RelativeLayout
            android:id="@+id/l4"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@+id/v1"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xinxi"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:src="@drawable/xinxi"></ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/xinxi"
                android:gravity="center"
                android:text="个人信息"
                android:textColor="#101010"
                android:textSize="18dp"></TextView>

            <ImageView
                android:id="@+id/wI2"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="22dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/button1"></ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginTop="1px"
            android:background="#CECECE"
            android:id="@+id/v2"
            android:layout_below="@+id/l4"
            />

        <RelativeLayout
            android:id="@+id/l5"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@+id/v2"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/dizhi"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:src="@drawable/dizhi"></ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/dizhi"
                android:gravity="center"
                android:text="我的地址"
                android:textColor="#101010"
                android:textSize="18dp"></TextView>

            <ImageView
                android:id="@+id/wI3"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="22dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/button1">

            </ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginTop="1px"
            android:background="#CECECE"
            android:id="@+id/v3"
            android:layout_below="@+id/l5"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/l6"
            android:orientation="horizontal"
            android:layout_below="@+id/v3">

            <ImageView
                android:id="@+id/mima"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:src="@drawable/mima"></ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_toRightOf="@+id/mima"
                android:gravity="center"
                android:text="修改密码"
                android:textColor="#101010"
                android:textSize="18dp"></TextView>
            <ImageView
                android:id="@+id/wI4"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_marginTop="22dp"
                android:src="@drawable/button1">

            </ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#CECECE"
            android:layout_marginTop="2px"
            android:id="@+id/v4"
            android:layout_below="@+id/l6"
            />
    </RelativeLayout>

</FrameLayout>